{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.6.2"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|jpg|jpeg|png|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ]
}

